{
  "name": "No More Referrer",
  "short_name": "nomorereferrer",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Delete referrer",
  "homepage_url": "https://github.com/noraworld/no-more-referrer",

  "permissions": [
    "activeTab", "storage"
  ],

  "content_scripts": [
    {
      "all_frames": true,
      "matches": ["<all_urls>"],
      "js": [
        "src/inject.js"
      ]
    }
  ]
}
